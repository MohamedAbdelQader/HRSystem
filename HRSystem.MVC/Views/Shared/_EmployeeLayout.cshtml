                                        <div class="dropdown-divider"></div><a asp-action="SignOut" asp-controller="HR" class="dropdown-item"><i class="ft-power mr-2"></i><span>Logout</span></a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

        </nav>

